{
  "name": "ui",
  "version": "1.0.0",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "type": "module",
  "license": "MIT",
  "types": "dist/es/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "serve": "node --experimental-specifier-resolution=node --loader ts-node/esm dev/dev.ts",
    "build": "yarn run build:es && yarn run build:cjs",
    "build:es": "rimraf dist/es && cross-env FORMAT=es node --experimental-specifier-resolution=node --loader ts-node/esm build/build.ts",
    "build:cjs": "rimraf dist/cjs && cross-env FORMAT=cjs  node --experimental-specifier-resolution=node --loader ts-node/esm build/build.ts",
    "test": "jest --coverage",
    "stylelint": "stylelint src/**/*.{less,vue} --fix",
    "eslint": "eslint --ext .jsx,.js,.vue,.ts,.tsx src --fix"
  },
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/es/index.d.ts"
    },
    "./dist/es": "./dist/es/index.js",
    "./dist/cjs": "./dist/cjs/index.js",
    "./dist/es/": "./dist/es/",
    "./dist/cjs/": "./dist/cjs/",
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/generator": "^7.17.10",
    "@babel/helper-module-imports": "^7.16.7",
    "@babel/parser": "^7.17.10",
    "@babel/preset-env": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/template": "^7.16.7",
    "@babel/traverse": "^7.17.10",
    "@babel/types": "^7.17.10",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/jest": "^27.5.1",
    "@types/lodash-es": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/eslint-config-standard": "^6.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.21",
    "@vue/vue3-jest": "^28.0.0",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^28.1.0",
    "chokidar": "^3.5.3",
    "commitlint": "^17.0.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.7",
    "eslint": "^8.14.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "less": "^4.1.2",
    "lint-staged": "^12.4.1",
    "lodash-es": "^4.17.21",
    "postcss": "^8.4.13",
    "postcss-html": "^1.4.1",
